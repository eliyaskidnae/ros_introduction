<launch>
    <arg name="x" default="0"/>
    <arg name="y" default="0"/>
    <arg name="z" default="0.5"/>
    <arg name="Y" default="0.76"/>
    <arg name="model" default="burger" doc="model type [burger, waffle,
    waffle_pi]"/>
    <param name="robot_description" command="$(find xacro)/xacro --inorder $(find
    turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />
    <!-- push robot_description to factory and spawn robot in gazebo -->
    <node name="turtlebot3_spawner" pkg="gazebo_ros" type="spawn_model" args="-x $(arg x) -y $(arg y) -z $(arg z) -Y $(arg Y) -urdf -model
    'turtlebot3' -param robot_description"
    respawn="false" output="screen"/>
</launch>